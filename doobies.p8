--update

function update_game()
 t+=1
 if (t%100==0) then
  if (p_s==2) then p_s=1 else p_s=2 end
 end
 if (t%50==0) then 
  if (cust.s==2) then cust.s=1 else cust.s=2 end
 end   
 if (t%50==0) then 
  do_time()
  if (custdelay<=0) then
  	if (cust.mode<1) then
  	 roll_cust()
  	 --do more here
  	 custdelay=flr(rnd(200)+10)
  	end
  else
   custdelay-=1
  end
 end
 if (t%20==0) then 
  signflicker=flr(rnd(400))
 end
 if (delay>-2) then delay-=1 end
 if (delay<1) then
  if (_drw==draw_game) then
 	 do_move()
 	 do_cust_ai()
 	elseif (_drw==draw_menu) then
 	 do_menu()
 	end 
 end
end

function do_time()
	min2+=1
	if (min2>=10) then
		min1+=1
		min2=0
	end
	if (min1>=6) then
		hours+=1
		min1=0
	end
	if(hours>=13) then
	 ampm+=1
		hours=1
	end
	if (ampm==3) then
		ampm=1
	end
end

function do_move()
 local x=nil
	if btn(0) then
	delay=20
	 x=mget(p_x-1,p_y)
		if (fget(x,0)==false) then	 
    p_x -= 1
  end
 elseif btn(1) then
 delay=20
  x=mget(p_x+1,p_y)
		if (fget(x,0)==false) then	 
    p_x += 1
  end
 elseif btn(2) then
 delay=20
  x=mget(p_x,p_y-1)
		if (fget(x,0)==false) then	 
    p_y -= 1
  end  
 elseif btn(3) then
 delay=20
  x=mget(p_x,p_y+1)
		if (fget(x,0)==false) then	 
    p_y += 1
  end
 end
 
 x=mget(p_x,p_y)
 if (p_hold<1) then
  p_text=""
 else
  if (cust.cook<10) then 
   p_text="holding: "..ingreduncooked[p_hold]
  elseif (cust.cook<100) then
   p_text="holding: "..ingredcooked[p_hold]
  else
   p_text="holding: "..item[p_hold]
  end
 end
 if (fget(x,1)==true) then
 	if (open==2) then
 		loctext="press 'z' to open shop"
 	else
 	 loctext="press 'z' to close shop"
 	end
 elseif (fget(x,2)==true) then
  if (t%20==0) then
   hours+=1
   if (custdelay>20) then
   	custdelay-=20
   end
  end
  loctext="the toilet is out of order"
 elseif (fget(x,3)==true) then
  if (p_hold>0) then
   p_text="cooking: "..item[p_hold]
  end
  if (p_hold>0) then
   if (cust.cook<100) and (t%10==0) then
    cust.cook+=1
    stove-=0.25
    if (stove<0) then stove=0 end
    if (stove>75) then
     cust.cook+=3
    elseif (stove>40) then
     cust.cook+=1
    end
   end
   loctext="%"..cust.cook.." cooked"
  else
   loctext="the stove is "..flr(stove).."% clean"
   if (open==2) then
    if (t%50==0) then
     if (stove<100) then
      stove+=1
     end
    end
   end
  end
 elseif (fget(x,4)==true) then
  loctext="refrigerator"
  if (cust.mode==1) and (open==1) then
   p_hold=cust.want
  end
 elseif (fget(x,5)==true) then
  loctext="prezz 'z' to change menu"
 elseif (fget(x,6)==true) then
  if (p_hold>0) then
   loctext="press 'z' to serve customer"
  else
   if not (cust.mode==0) then
    loctext=served.." customers served"
   else
    loctext="cash register"
   end
  end
	else
	 loctext=""
 end
 
 if btn(4) then
 delay=20
 	if (fget(x,1)==true) then
 		if (open==1) then
 			open=2
 		else
 		 open=1
 		end
 	end	
 	if (fget(x,6)==true) then
 	 if (cust.mode==1) then
 	 	p_hold=0
 	 	cust.mode+=1
 		end
 	end	
 	if (fget(x,5)==true) then
 		_drw=draw_menu
 	end	
 end
end

function do_menu()
 local x=(menuselect/2)-1
	if btn(0) then
	delay=20
				price[x]-=1
 elseif btn(1) then
 delay=20
				price[x]+=1
 elseif btn(3) then
 delay=20
  if (menuselect < 12) then
   menuselect+=2
  else 
   menuselect=4
  end
 elseif btn(2) then
 delay=20
  if (menuselect > 4) then
   menuselect-=2
  else 
   menuselect=12
  end
 elseif btn(4) then
 delay=20
  _drw=draw_game
 end
end


