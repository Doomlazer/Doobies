function _init()
 _upd=update_game
 _drw=draw_game

 item={
 "pickens county tornado",
 "beans & onion slab",
 "slaw dog",
 "cheese burger",
 "dr. pepper cake"
 }
 ingreduncooked={"raw hotdog & bun",
 "cold beans & onion",
 "raw dog & cold slaw",
 "raw hamburger & bun",
 "eggs, flour & soda"
	}
	ingredcooked={"warm hotdog & bun",
 "warm beans & onion",
 "warm dog & cold slaw",
 "rare hamburger & bun",
 "cake batter"
	}
 price={15,8,3,9,20}
 
 delay=20
 
 room=2
 served=0
 
 open=1
 signflicker=1
 hours=4
 min1=0
 min2=0
 ampm=1
 ap={'am','pm'}
 
 p_ani={49,50}
 p_x=6
 p_y=4
 p_s=1
 p_hold=0
 p_text=""
 t=0
 
 cust={}
 cust.ani={}
 roll_cust()
 custdelay=0
 
 stove=100
 loctext=""
 stattext1=""
 stattext2=""
 stattext3=""
 s1col=6
 s2col=7
 s3col=11
 
 menuselect=4
 
	money=100
 order=1

end

function _update60()
 _upd()
end

function _draw()
